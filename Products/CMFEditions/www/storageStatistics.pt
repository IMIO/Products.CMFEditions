<h2>Storage Statistics</h2>

<form action="edit" method="post">

  <table border="0">

    <tr>
      <th align="left">
        history id
      </th>
      <th align="left">
        versions
      </th>
      <th align="left">
        current path
      </th>
    </tr>

    <tr tal:repeat="histData here/portal_historiesstorage/zmi_getHistoriesData">
